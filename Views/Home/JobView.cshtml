
@{
    ViewBag.Title = "JobView";
}



<div style="margin-left:200px;margin-top:50px">
    <h1 >Job </h1>
       
    <div class="row" style="margin-top:70px">
        <div class="input-lg">
            <label class="col-md-3"> Job Title</label>


            <label class="col-md-8 " style="border-radius:5px" >@ViewBag.Jobtitle</label>

        </div>
    </div>


    <div class="row" style="margin-top:30px">
        <div class="input-lg">
            <label class="col-md-3"> Job Duration</label>

            <input type="text" class="col-md-1" style="border-radius:5px" />
            <label class="col-md-1">To</label>
            <input type="text" class="col-md-1" style="border-radius:5px" />
        </div>
    </div>
    <div class="row" style="margin-top:30px">
        <div class="input-lg">
            <label class="col-md-3"> Professional Required</label>

            <label class="col-md-8 " style="border-radius:5px">@ViewBag.professional</label>
        </div>
    </div>
    <div class="row" style="margin-top:30px">
        <div class="input-lg">
            <label class="col-md-3"> Job Location</label>

            <label class="col-md-8 " style="border-radius:5px">@ViewBag.Joblocation</label>
        </div>
    </div>
    <div class="row" style="margin-top:30px">
        <div class="input-lg">
            <label class="col-md-3"> Salary Range</label>

            <label class="col-md-8 " style="border-radius:5px">@ViewBag.salary</label>
        </div>
    </div>
    <div class="row" style="margin-top:30px">
        <div class="input-lg">
            <label class="col-md-3"> Job Description</label>
            <label class="col-md-8 " style="border-radius:5px">@ViewBag.Jobdescription</label>

           
        </div>
    </div>

   
    <div>
        <a href="Proposal" class="col-md-offset-3 btn btn-primary">Write Proposal</a>
        <a href="profile" class="btn btn-primary  " >View Client Profile</a>
    </div>
</div>
